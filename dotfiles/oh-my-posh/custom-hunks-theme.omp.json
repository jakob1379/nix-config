{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 222,
  "final_space": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#c44569",
          "leading_diamond": "\ue0b6",
          "properties": {
            "windows": "\ue62a"
          },
          "template": "{{ if .Env.CONTAINER_ID }}\uebca {{ if eq .Env.CONTAINER_ID \"almalinux\" }}\uf31d{{ else if eq .Env.CONTAINER_ID \"alpine\" }}\uf300{{ else if eq .Env.CONTAINER_ID \"android\" }}\uf17b{{ else if eq .Env.CONTAINER_ID \"aosc\" }}\uf301{{ else if eq .Env.CONTAINER_ID \"arch\" }}\uf303{{ else if eq .Env.CONTAINER_ID \"centos\" }}\uf304{{ else if eq .Env.CONTAINER_ID \"coreos\" }}\uf305{{ else if eq .Env.CONTAINER_ID \"debian\" }}\uf306{{ else if eq .Env.CONTAINER_ID \"deepin\" }}\uf321{{ else if eq .Env.CONTAINER_ID \"devuan\" }}\uf307{{ else if eq .Env.CONTAINER_ID \"elementary\" }}\uf309{{ else if eq .Env.CONTAINER_ID \"endeavouros\" }}\uf322{{ else if eq .Env.CONTAINER_ID \"fedora\" }}\uf30a{{ else if eq .Env.CONTAINER_ID \"gentoo\" }}\uf30d{{ else if eq .Env.CONTAINER_ID \"mageia\" }}\uf310{{ else if eq .Env.CONTAINER_ID \"manjaro\" }}\uf312{{ else if eq .Env.CONTAINER_ID \"mint\" }}\uf30e{{ else if eq .Env.CONTAINER_ID \"nixos\" }}\uf313{{ else if eq .Env.CONTAINER_ID \"opensuse-tumbleweed\" }}\uf314{{ else if eq .Env.CONTAINER_ID \"opensuse\" }}\uf314{{ else if eq .Env.CONTAINER_ID \"raspbian\" }}\uf315{{ else if eq .Env.CONTAINER_ID \"redhat\" }}\uf316{{ else if eq .Env.CONTAINER_ID \"rocky\" }}\uf32b{{ else if eq .Env.CONTAINER_ID \"sabayon\" }}\uf317{{ else if eq .Env.CONTAINER_ID \"slackware\" }}\uf319{{ else if eq .Env.CONTAINER_ID \"ubuntu\" }}\uf31b{{ else }}󰅩{{ end }}{{ else }}{{ if .WSL }}WSL at {{ end }}{{ .Icon }}{{ end }}"
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#c44569",
          "template": "{{ .Path }} ",
          "properties": {
            "style": "full"
          }
        },
        {
          "type": "nix-shell",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#C2DDEF",
          "template": "{{ if not (eq .Type \"unknown\") }} \uf313 nix-{{ .Type }}{{ end }}",
          "properties": {
            "style": "full"
          }
        },
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#f78fb3",
          "template": " \uf313 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} "
        },
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#f78fb3",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }}{{ end }}{{ .Full }}{{ end }} ",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#caffbf",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FCA17D{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#89d1dc{{ end }}",
            "{{ if gt .Behind 0 }}#f17c37{{ end }}"
          ],
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }} ",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "status",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#ffffff",
          "background": "#00897b",
          "background_templates": ["{{ if .Error }}#e91e63{{ end }}"],
          "template": "{{ if eq .Code 0 }}\uf00c{{ else }}\uf071 {{ reason .Code }}{{ end }} ",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "type": "executiontime",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#ffffff",
          "background": "#83769c",
          "template": " {{ .FormattedMs }} ",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#FFD700",
          "background": "#86BBD8",
          "template": " ⚡"
        },
        {
          "type": "session",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#ffffff",
          "background": "#86BBD8",
          "template": " {{ .UserName }} "
        },
        {
          "type": "shell",
          "style": "diamond",
          "invert_powerline": true,
          "foreground": "#ffffff",
          "background": "#33658A",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .Name }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "root",
          "style": "plain",
          "template": " \uf0e7 "
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#ffffff",
          "template": "<#69FF94>❯</>"
        }
      ],
      "newline": true
    }
  ]
}
